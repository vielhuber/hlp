        "@babel/preset-react": "^7.23.3",
        "@babel/runtime": "^7.23.4",
        "@prettier/plugin-php": "^0.21.0",
        "autoprefixer": "^10.4.16",
        "babel-core": "^7.0.0-bridge.0",
