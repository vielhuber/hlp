{
    "name": "hlp",
    "version": "2.3.7",
    "main": "_js/_build/script.js",
    "files": [
        "_js/_build/*.js",
        "hlp.js"
    ],
    "scripts": {
        "js:browserify": "browserify ./_js/script.js --outfile ./hlp.js --transform babelify",
        "js:uglify": "uglifyjs --compress --mangle --output ./hlp.js ./hlp.js",
        "js:babel": "npx babel ./_js/script.js --out-dir ./_js/_build/",
        "js:test": "npm-run-all --sequential js:test-babel js:test-jest",
        "js:test-babel": "browserify ./_tests/_js/script.js --outfile ./_tests/_build/bundle.test.js --transform babelify",
        "js:test-jest": "jest --verbose --noStackTrace --no-cache || true",
        "js": "npm-run-all --sequential js:browserify js:uglify js:babel js:test",
        "watch:js": "onchange ./_js/*.js --initial --wait --delay 500 --await-write-finish -- onerror \"npm run js --silent\" --title \"watch:js\" --message \"build failed\" --sound mute",
        "watch:js-test": "onchange ./_tests/_js/*.js --wait --delay 500 --await-write-finish -- onerror \"npm run js:test --silent\" --title \"watch:js-test\" --message \"build failed\" --sound mute",
        "build": "npm-run-all --parallel js",
        "dev": "npm-run-all --parallel watch:*"
    },
    "repository": "git@github.com:vielhuber/hlp.git",
    "author": "David Vielhuber <david@vielhuber.de>",
    "license": "MIT",
    "dependencies": {
